        <div class="row mb-2">
            <div class="col-sm-6">
                <h4>Hareketlerim</h4>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="~/Home/Index">Ana Sayfa</a></li>
                    <li class="breadcrumb-item active">Hareketlerim</li>
                </ol>
            </div><!-- /.col -->
        </div><!-- /.row -->
    </div><!-- /.container-fluid -->
</div>
<!-- /.content-header -->
