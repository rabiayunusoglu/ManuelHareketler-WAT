                                                    <span class="fas fa-sort-alpha-down-alt" ng-show="sortColumn4=='productCode'" ng-class="{'fas fa-sort-alpha-up-alt': !reverse4, 'fas fa-sort-alpha-down-alt': reverse4}"></span>
                                                </th>
                                                <th ng-click="sortBy4('productCodeInfo')">
                                                    Motor Bilgisi
                                                    <span class="fas fa-sort-alpha-down-alt" ng-show="sortColumn4=='productCodeInfo'" ng-class="{'fas fa-sort-alpha-up-alt': !reverse4, 'fas fa-sort-alpha-down-alt': reverse4}"></span>
                                                </th>
                                                <th ng-click="sortBy4('productYear')">
                                                    Yıl
                                                    <span class="fas fa-sort-alpha-down-alt" ng-show="sortColumn4=='productYear'" ng-class="{'fas fa-sort-alpha-up-alt': !reverse4, 'fas fa-sort-alpha-down-alt': reverse4}"></span>
                                                </th>
                                                <th ng-click="sortBy4('registerDate')">
                                                    Kayıt Tarihi
                                                    <span class="fas fa-sort-alpha-down-alt" ng-show="sortColumn4=='registerDate'" ng-class="{'fas fa-sort-alpha-up-alt': !reverse4, 'fas fa-sort-alpha-down-alt': reverse4}"></span>
                                                </th>
                                                <th ng-click="sortBy4('loginDate')">
                                                    Giriş Tarihi
                                                    <span class="fas fa-sort-alpha-down-alt" ng-show="sortColumn4=='loginDate'" ng-class="{'fas fa-sort-alpha-up-alt': !reverse4, 'fas fa-sort-alpha-down-alt': reverse4}"></span>
                                                </th>
                                                <th ng-click="sortBy4('loginDateTime')">
                                                    Saat
                                                    <span class="fas fa-sort-alpha-down-alt" ng-show="sortColumn4=='loginDateTime'" ng-class="{'fas fa-sort-alpha-up-alt': !reverse4, 'fas fa-sort-alpha-down-alt': reverse4}"></span>
                                                </th>
                                                <th>
                                                    Hareket Sebebi
                                                </th>
                                                <th>
                                                    Açıklama
                                                </th>
                                                <th>
                                                    İncele
                                                </th>
                                            </tr>
                                        </thead>
                                        <tbody ng-repeat="l in finishedList| orderBy: sortColumn4: reverse4 | limitTo: itemsPerPage4: (currentPage4 - 1) * itemsPerPage4  |filter:searchText4">
                                            <tr ng-if="!finishedList || finishedList.length == 0">
                                                <td>
                                                    <h3>Tablo Boş </h3>
                                                </td>
                                            </tr>
                                            <tr ng-if="l.statusType==3" style="background: #B4DE86 ">
                                                <td>
                                                    {{l.id}}
                                                </td>
                                                <td>
                                                    {{l.statusName}}
                                                </td>
                                                <td>
                                                    {{l.userRegisterNo}}
                                                </td>
                                                <td>
                                                    {{l.username}}
                                                </td>

                                                <td>
                                                    {{l.UY}}
                                                </td>
                                                <td>
                                                    {{l.HTU}}
                                                </td>
                                                <td>
                                                    {{l.material}}
                                                </td>
                                                <td>
                                                    {{l.materialText}}
                                                </td>
                                                <td>
                                                    {{l.MT}}
                                                </td>
                                                <td>
                                                    {{l.amount}}
                                                </td>
                                                <td>
                                                    {{l.brm}}
                                                </td>
                                                <td>
                                                    {{l.priceBrm}}
                                                </td>
                                                <td>
                                                    {{l.total}}
                                                </td>
                                                <td>
                                                    {{l.DnmsMF}}
                                                </td>
                                                <td>
                                                    {{l.DnmsMFValue}}
                                                </td>
                                                <td>
                                                    {{l.orderValue}}
                                                </td>
                                                <td>
                                                    {{l.productCode}}
                                                </td>
                                                <td>
                                                    {{l.productCodeInfo}}
                                                </td>
                                                <td>
                                                    {{l.productYear}}
                                                </td>
                                                <td>
                                                    {{l.registerDate}}
                                                </td>
                                                <td>
                                                    {{l.loginDate}}
                                                </td>
                                                <td>
                                                    {{l.loginDateTime}}
                                                </td>
                                                <td>
                                                    {{l.actionReason}}
                                                </td>
                                                <td>
                                                    {{l.reasonDetail}}
                                                </td>
                                                <td>
                                                    <button type="button" class="btn btn-info" data-toggle="modal" data-target="#modal-detail" ng-click="fetchHistory(l.manualActionID,l)">İncele</button>
                                                </td>
                                            </tr>

                                        </tbody>

                                    </table>
                                </div>
                                <div class="card-footer clearfix">
                                    <ul class="pagination pagination-sm m-0 float-right" uib-pagination total-items="finishedList.length" items-per-page="itemsPerPage4" ng-model="currentPage4" max-size="5" boundary-links="true" num-pages="numPages">
                                    </ul>
                                </div>
                            </div>
                        </div>
                        <div class="modal fade" id="modal-detail">
                            <div class="modal-dialog modal-xl">
                                <div class="modal-content">
                                    <div class="overlay dark" ng-if="!uploading1">
                                        <i class="fas fa-2x fa-sync-alt fa-spin"></i>
                                    </div>
                                    <div class="modal-header">
                                        <h4 class="modal-title">İnceleme</h4>
                                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                            <span aria-hidden="true">&times;</span>
                                        </button>
                                    </div>
                                    <div class="modal-body">
                                        <div class="row">
                                            <div class="col-md-12">
                                                <div class="timeline">
                                                    <div class="time-label">
                                                        <span class="bg-success">Başlangıç</span>
                                                    </div>
                                                    <div ng-repeat="x in historyList">
                                                        <i class="fas fa-comments bg-yellow"></i>
                                                        <div class="timeline-item">
                                                            <span class="time"><i class="fas fa-clock"></i>{{x.redirectedDate}}</span>
                                                            <h3 class="timeline-header"><a href="#">{{x.senderTeam}}-{{x.senderUser}}</a> kullanıcısı ilgili hareketi <a href="#">{{x.receiverTeam}}-{{x.receiverUser}}</a> yönlendirdi.</h3>

                                                            <div class="timeline-body">
                                                                {{x.redirectedText}}
                                                            </div>
                                                            <div class="timeline-footer">
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="time-label" ng-if="selectedItem.statusType==2">
                                                        <i class="fas fa-clock bg-gray"></i>
                                                    </div>
                                                    <div ng-if="selectedItem.statusType==3">
                                                        <i class="fas fa-comments bg-success"></i>
                                                        <div class="timeline-item">
                                                            <span class="time"><i class="fas fa-clock"></i>{{selectedItem.madeDateText}}</span>
                                                            <h3 class="timeline-header"><a href="#">{{selectedItem.username}}</a> <strong> ilgili hareketi tamamladı.</strong></h3>

                                                            <div class="timeline-body">
                                                                <strong> {{selectedItem.actionReason}} </strong> sebebinden dolayı manuel hareket yapılmıştır.
                                                                Açıklama: {{selectedItem.reasonDetail}}.
                                                            </div>
                                                            <div class="timeline-footer">
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="time-label" ng-if="selectedItem.statusType==3">
                                                        <span class="bg-success">Kapandı</span>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="modal-footer justify-content-between">
                                        <button type="button" class="btn btn-default" data-dismiss="modal">İptal</button>
                                        <button type="button" class="btn btn-success" data-dismiss="modal">Tamam</button>
                                    </div>
                                </div>

                                <!-- /.modal-content -->
                            </div>

                            <!-- /.modal-dialog -->
                        </div>

                        <!-- /.modal -->
                    </div>
                    <div class="overlay dark" ng-if="!uploading">
                        <i class="fas fa-2x fa-sync-alt fa-spin"></i>
                    </div>
                </div>
                <!-- /.row -->
            </div>
        </section>

    </div>

</div>


